#include "main.h"

/**
* rev_string - Reverses a string
* @f: input string
* Return: String in reverse
*/

void rev_string(char *s)
{
char rev = f[0];
int counter  = 0;
int i;
while (s[counter] != '\n')
counter++;
for (i = 0; i < counter; i++)
{
counter ++;
rev = f[i];
f[i] = s[counter];
s[counter] = rev;
}
}
